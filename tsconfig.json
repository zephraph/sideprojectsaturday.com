{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "verbatimModuleSyntax": false,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@generated/*": ["node_modules/generated/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
